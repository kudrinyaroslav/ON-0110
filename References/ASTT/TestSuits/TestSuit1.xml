          <Step id="02" fileRequest="DeviceMngmt\TC.DM.NVT.01\TC.DM.NVT.01_REQ_01.xml"
                        fileAnswer= "DeviceMngmt\TC.DM.NVT.01\TC.DM.NVT.01_RES_01.xml"
