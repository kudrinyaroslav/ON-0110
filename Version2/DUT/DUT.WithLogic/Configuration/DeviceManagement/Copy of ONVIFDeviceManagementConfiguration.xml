<?xml version="1.0" encoding="utf-8"?>
<ONVIFDeviceManagementConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <TimeOfset>0</TimeOfset>
  <TimeZone>UTC0:0</TimeZone>
  <DaylightSavings>1</DaylightSavings>
  <DateTimeType>Manual</DateTimeType>
  <ConfigurableScopes>
    <Scope>
      <ScopeDef xmlns="http://www.onvif.org/ver10/schema">Configurable</ScopeDef>
      <ScopeItem xmlns="http://www.onvif.org/ver10/schema">onvif://www.onvif.org/Location/Novgorod</ScopeItem>
    </Scope>
  </ConfigurableScopes>
  <RemoteDiscoveryMode>NonDiscoverable</RemoteDiscoveryMode>
  <DiscoveryMode>Discoverable</DiscoveryMode>
  <NotFixedUsers>
    <Item>
      <Key>
        <string>user</string>
      </Key>
      <Value>
        <User>
          <Username xmlns="http://www.onvif.org/ver10/schema">user</Username>
          <Password xmlns="http://www.onvif.org/ver10/schema">2</Password>
          <UserLevel xmlns="http://www.onvif.org/ver10/schema">User</UserLevel>
        </User>
      </Value>
    </Item>
  </NotFixedUsers>
  <HostnameInformation>
    <FromDHCP xmlns="http://www.onvif.org/ver10/schema">false</FromDHCP>
    <Name xmlns="http://www.onvif.org/ver10/schema">duthostname</Name>
  </HostnameInformation>
  <NTPFromDHCP>
    <NetworkHost>
      <Type xmlns="http://www.onvif.org/ver10/schema">IPv4</Type>
      <IPv4Address xmlns="http://www.onvif.org/ver10/schema">192.168.10.3</IPv4Address>
    </NetworkHost>
  </NTPFromDHCP>
  <NTPInformation>
    <FromDHCP xmlns="http://www.onvif.org/ver10/schema">false</FromDHCP>
    <NTPManual xmlns="http://www.onvif.org/ver10/schema">
      <Type>IPv4</Type>
      <IPv4Address>192.168.10.3</IPv4Address>
    </NTPManual>
  </NTPInformation>
  <HostnameFromDHCP>hostnamefromDHCP</HostnameFromDHCP>
</ONVIFDeviceManagementConfiguration>