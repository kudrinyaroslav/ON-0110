            <tt:AudioEncoderConfiguration token="audio_encoder1">
              <tt:Name>audio_encoder</tt:Name>
              <tt:UseCount>1</tt:UseCount>
              <tt:Encoding>G711</tt:Encoding>
              <tt:Bitrate>64</tt:Bitrate>
